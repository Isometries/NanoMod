#!@MODPATH@/system/bin/bash

source "${MODPATH}/system/bin/nanomod-overlay.functions"
OVERLAY_APPS=$(cat "${config}")
clear_overrides

for app in ${OVERLAY_APPS}; do
	if [ -d /system/app/${app} ]; then
		add_override "app/${app}"
	elif [ -d /system/priv-app/${app} ]; then
		add_override "priv-app/${app}"
	else	echo "app \"${app}\" is not installed"
	fi
done
